<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">  
        <shape>  
            <corners android:radius="2.0dip" />  
            <gradient android:startColor="@color/progressdefulcolor" android:centerColor="@color/progressdefulcolor" android:endColor="@color/progressdefulcolor" android:angle="270.0" android:centerY="2.0"  />  
        </shape>  
    </item>  
    <item android:id="@android:id/progress">  
        <clip>  
            <shape>  
                <corners android:radius="2.0dip" />  
                <gradient android:startColor="@color/progressstartcolor" android:centerColor="@color/progressstartcolor" android:endColor="@color/progressstartcolor" android:angle="270.0" android:centerY="2.0"  />  
            </shape>  
        </clip>  
    </item>  

</layer-list>
